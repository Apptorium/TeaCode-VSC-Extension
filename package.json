{
    "name": "teacode-vsc-helper",
    "displayName": "TeaCode VSC Helper",
    "description": "Provides integration with TeaCode http://apptorium.com/teacode",
    "icon": "images/TeaCode-Icon.png",
    "version": "1.1.2",
    "publisher": "Apptorium",
    "repository": {
        "type": "git",
        "url": "https://github.com/Apptorium/TeaCode-VSC-Extension"
	},
	"engines": {
        "vscode": "^1.38.0"
    },
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.expand"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.expand",
				"title": "Expand with TeaCode"
			}
		],
		"keybindings":[
			{
				"command": "extension.expand",
				"key": "cmd+ctrl+E",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	}
}
